@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- The Modal -->
<div class="modal fade" id="myModal">
    <div class="modal-dialog modal-xl" style="width:70%;">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">MACHINE STATUS</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body" style="margin:auto">
                <div class="row mb-2">
                    <div class="col-4">
                       
                        <label>DATE/TIME:</label>
                        <input name='date' id='date' class='form-control form-control-sm' type='text' value='' readonly style='background-color: #f3f3f3;' />
                        <span>MACHINE PLATFORM:</span>
                        <input name='machinePF' id='machinePlatform' class='form-control form-control-sm' type='text' value='' readonly style='background-color: #f3f3f3;' />
                        <span>MACHINE ID:</span>
                        <input name='machineID' id='machineID' class='form-control form-control-sm' type='text' value='' readonly style='background-color: #f3f3f3;' />
                        <div hidden><input id="status_owner_track" /></div>
                        <span>STATUS OWNER:</span>
                        <select id='stsOwner' name='stsOwner' class='form-control form-control-sm'></select>
                        <input id="status_track" hidden />
                        <span>STATUS:</span>
                        <select id='stsDes' name='stsDes' class='form-control form-control-sm'></select>
                        <span>LOT NO:</span>
                        <input class='form-control form-control-sm' id='lotNo' name='lotNo' type="text" maxlength="9" />
                        <span>FAMILY NAME:</span>
                        <select id='prodName' name='prodName' class='form-control form-control-sm'></select>
                        <div hidden>
                            <select name='uph' id='uph_select' class='form-control form-control-sm' disabled></select>
                        </div>
                        <div hidden>
                            <input id="ipcc-value" />
                        </div>
                        <div hidden>
                            <input id="ipcc-reset" />
                        </div>
                        <span>PACKAGE TYPE:</span>
                        <select id='pkgType' name='pkgType' readonly class='form-control form-control-sm' disabled></select>
                    </div>
                    <div class="col-4">
                        <span style="font-size:16px;">IMPENDANCE:</span>
                        <div class="container" style="border:solid; border-color:#ebebe0; border-width:2px; font-size:14px;">
                            <span>BH1</span>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                            <div class="row mb-2">
                                <div class="col-4">
                                    <span>USL:</span>
                                    <input id="usl_1_impendance" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>NOMINAL:</span>
                                    <input id="nominal_1_impendance" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>LSL:</span>
                                    <input id="lsl_1_impendance" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                            </div>
                            <span>BH2</span>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                            <div class="row mb-2">
                                <div class="col-4">
                                    <span>USL:</span>
                                    <input id="usl_2_impendance" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>NOMINAL:</span>
                                    <input id="nominal_2_impendance" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>LSL:</span>
                                    <input id="lsl_2_impendance" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                            </div>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                        </div>
                        <span style="font-size:16px;">BONDING FORCE IGBT:</span>
                        <div class="container" style="border:solid; border-color:#ebebe0; border-width:2px; font-size:14px;">
                            <span>BH1</span>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                            <div class="row mb-2">
                                <div class="col-4">
                                    <span>USL:</span>
                                    <input id="usl_1_bf_igbt" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>NOMINAL:</span>
                                    <input id="nominal_1_bf_igbt" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>LSL:</span>
                                    <input id="lsl_1_bf_igbt" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                            </div>
                            <span>BH2</span>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                            <div class="row mb-2">
                                <div class="col-4">
                                    <span>USL:</span>
                                    <input id="usl_2_bf_igbt" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>NOMINAL:</span>
                                    <input id="nominal_2_bf_igbt" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>LSL:</span>
                                    <input id="lsl_2_bf_igbt" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                            </div>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                        </div>
                    </div>
                    <div class="col-4">
                        <span style="font-size:16px;">BONDING FORCE MID STITCH:</span>
                        <div class="container" style="border:solid; border-color:#ebebe0; border-width:2px; font-size:14px;">
                            <span>BH1</span>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                            <div class="row mb-2">
                                <div class="col-4">
                                    <span>USL:</span>
                                    <input id="usl_1_bf_mid_stitch" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>NOMINAL:</span>
                                    <input id="nominal_1_bf_mid_stitch" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>LSL:</span>
                                    <input id="lsl_1_bf_mid_stitch" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                            </div>
                            <span>BH2</span>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                            <div class="row mb-2">
                                <div class="col-4">
                                    <span>USL:</span>
                                    <input id="usl_2_bf_mid_stitch" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>NOMINAL:</span>
                                    <input id="nominal_2_bf_mid_stitch" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>LSL:</span>
                                    <input id="lsl_2_bf_mid_stitch" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                            </div>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                        </div>
                        <span style="font-size:16px;">BONDING FORCE LEAD:</span>
                        <div class="container" style="border:solid; border-color:#ebebe0; border-width:2px; font-size:14px;">
                            <span>BH1</span>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                            <div class="row mb-2">
                                <div class="col-4">
                                    <span>USL:</span>
                                    <input id="usl_1_bf_lead" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>NOMINAL:</span>
                                    <input id="nominal_1_bf_lead" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>LSL:</span>
                                    <input id="lsl_1_bf_lead" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                            </div>
                            <span>BH2</span>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                            <div class="row mb-2">
                                <div class="col-4">
                                    <span>USL:</span>
                                    <input id="usl_2_bf_lead" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>NOMINAL:</span>
                                    <input id="nominal_2_bf_lead" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                                <div class="col-4">
                                    <span>LSL:</span>
                                    <input id="lsl_2_bf_lead" class="form-control form-control-sm ipcc_control" name="name" value=" " />
                                </div>
                            </div>
                            <div class="mb-3" style="border-bottom:solid;border-color:#34495E"></div>
                        </div>
                    </div>
                </div>
                @*//*@
                <div class="row mb-2">
                    <div class="col-12">
                        <span>REMARKS</span>
                        <textarea id='remarks' class="form-control form-control-sm" rows='4' name='remarks' style='width: 100%' maxlength='500'></textarea>
                    </div>
                </div>
                <div class="row mb-2">
                    <div class="col-12 offset-5">
                        <span>GROUP:</span>
                        <select id='group' name='group' class='form-control form-control-sm' style='width: 100px;'>
                            <option value=''>A</option>
                            <option value=''>B</option>
                            <option value=''>C</option>
                            <option value=''>RS</option>
                        </select>
                    </div>
                </div>
                <div class="row mb-2">
                    <div class="col-12">
                        <select name='user' id='user' class='form-control mt-2 mb-2' style="width:250px; margin:auto"></select>
                        <input type='password' name='pass' id='pass' class="form-control" placeholder='PASSWORD' style="width:250px; margin:auto">
                    </div>
                </div>


                <!-- Modal footer -->
                <div class="modal-footer">
                    <button id="track_in" class="btn btn-secondary">TRACK IN</button>
                    <button id="track_out" class="btn btn-secondary">TRACK OUT</button>
                    <button id='save' type="button" class="btn btn-success">UPDATE</button>
                </div>

            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="downloadModal" tabindex="-1" role="dialog">
